{% extends 'layouts/base.html.twig' %}

{% block title %}Songterest
{% endblock %}

{% block body %}
	<h1>{{ songs|length }}
		Songs</h1>

	{% for song in songs %}
		<article>
			<h2>
				<a href="{{ path('app_songs_show', {id:song.id})}}">{{ song.title ~ " - " ~ song.artist}}</a>
			</h2>
		</article>
	{% endfor %}
{% endblock %}
